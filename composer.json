{
  "name": "aaqibshahzad/scalekit",
  "description": "A SaaS-ready Laravel starter kit with Sanctum auth, optional tenancy, OAuth, and role management.",
  "keywords": ["laravel", "starter-kit", "saas", "tenancy", "oauth", "sanctum", "spatie"],
  "type": "library",
  "license": "MIT",
  "author": [{
    "name": "Aqib Shahzad",
    "email": "aqitech99@gmail.com"
  }],
  "autoload": {
    "psr-4": {
      "AaqibShahzad\\ScaleKit\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AaqibShahzad\\ScaleKit\\ScaleKitServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=8.4",
    "laravel/framework": "^12",
    "laravel/sanctum": "^4.0",
    "laravel/socialite": "^5.0",
    "spatie/laravel-permission": "^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "pestphp/pest": "^4.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}